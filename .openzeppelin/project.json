{
  "manifestVersion": "2.2",
  "contracts": {
    "ADIToken": "ADIToken",
    "DocExchange": "DocExchange",
    "IterableDocExchange": "IterableDocExchange"
  },
  "dependencies": {
    "@openzeppelin/contracts-ethereum-package": "^3.0.0"
  },
  "name": "ledger-academy",
  "version": "1.0.0",
  "compiler": {
    "compilerSettings": {
      "optimizer": {
        "enabled": false,
        "runs": "200"
